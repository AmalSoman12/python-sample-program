
a = float(input("Enter the value of a: "))
b = float(input("Enter the value of b: "))

left_side = (a + b) ** 2
right_side = a**2 + 2*a*b + b**2


print("The equation holds true." if left_side == right_side else "The equation does not hold true.")

print(f"Left side (a+b)^2: {left_side}")
print(f"Right side a^2 + 2ab + b^2: {right_side}")
